/* pages/index/index.wxss */
page{
  height: 100%;
}
.space{
  height: 100vw;
  width: 100%;
}
.body{
  height: 100%;
  background: #ddd;
  display: flex;
  flex-direction: column;
  overflow: hidden;
}
scroll-view{
  height: calc(100% - 88rpx);
}
.nav{
  width:100%;
  height:100rpx;
  display:flex;
  justify-content: space-around;
}
.nav>navigator{
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}
.middle-banner{
  padding:40rpx 32rpx 0;
  background: #fff;
}
.middle-banner image{
  width: 100%;
}