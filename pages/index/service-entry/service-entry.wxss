.service-wrap{
  height:64rpx;
  width: 1005;
  display: flex;
  justify-content: center;
  padding:40rpx 32rpx 0;
  background: #fff;
}
.service-body{
  width: 100%;
  height: 100%;
  background: hsla(0,0%,96%,.53);
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;

}
.left{
  color: #666;
  font-size: 24rpx;
}
.right{

  display: flex;  
}
.right-item{
  color: #999;
  font-size: 24rpx;
}
.line{
  height: 24rpx;
  width: 1rpx;
  background-color: #e6e6e6;
  margin: 0 12px;
}